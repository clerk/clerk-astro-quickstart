---
import { Show } from '@clerk/astro/components'
---

<!doctype html>
<html>
  <body>
    <Show
      when={{ permission: 'premium_access:manage' }}
      fallback={(<p>Only subscribers with the Premium Access Manage permission can access this content.</p>)}
    >
      <h1>Exclusive Management Content</h1>
      <p>This content is only visible to users with Premium Access Manage permission.</p>
    </Show>
  </body>
</html>
